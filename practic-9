#include <iostream>
using namespace std;
int main()
{
    int a;
    int n = 0;
    int b = 0;
    cin >> a;
    if (a > 15) {
        cout << "error";
    }
    else {
        while (b != a) {
            if (a > 15) {
                break;
            }
            else {
                while (b != a) {
                    cout << "done 1 circle" << endl;
                    b++;
                }
                b = a - b;
                cin >> a;
            }
        }
       }
    }
